# Environment variables for Product Data Explorer backend

# Server port
PORT=3000

# Redis connection
REDIS_HOST=localhost
REDIS_PORT=6379

# Database connection string
DATABASE_URL=postgresql://user:password@localhost:5432/product_data_explorer

# Admin API key for protected endpoints
ADMIN_API_KEY=your-secure-api-key-here

# Scrape job TTL in seconds (default 24 hours)
SCRAPE_TTL_SECONDS=86400

# Node environment
NODE_ENV=development
